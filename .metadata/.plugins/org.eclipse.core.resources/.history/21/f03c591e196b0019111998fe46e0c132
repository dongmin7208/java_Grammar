package day19;
class Info{
	String name;
	int age;
	Info(String name, int age){
		this.name = name; this.age = age;
	}
	public String toString() {
		return "[" + name + ", "+ age + "]";
	}
}
public class Ex01 {
	public static void main(String[] args) {
		Info[] arr = new Info[3];
		arr[0] = new Info("È«±æµ¿", 20);
		arr[1] = new Info("È«±æµ¿", 20);
		arr[2] = new Info("È«±æµ¿", 20);
	}
}
