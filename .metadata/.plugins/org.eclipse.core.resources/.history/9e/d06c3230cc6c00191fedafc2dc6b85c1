package day21;

import java.awt.Frame;
import java.awt.Point;

public class Ex01 {
	public static void main(String[] args) {
		Frame f = new Frame();
		
		f.setSize(400, 500);
		Point loc = new Point();
		loc.x = 200;
		loc.y = 500;
		f.setLocation(loc);
		f.setResizable(false);
		f.setVisible(true);
	}
}
